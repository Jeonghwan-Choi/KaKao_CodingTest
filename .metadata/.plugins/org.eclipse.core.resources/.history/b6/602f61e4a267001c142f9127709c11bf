package com.Cote;

public class String_Compression {

	public static void main(String[] args) {
		String s = "ababcdcdababcdcd";
		int answer = 0;
		
//		for(int i=0;i<s.length();i++) {
//			
//
//		}

	    String temp;
	    while (true) {
	        temp = s.replaceAll("(.+)\\1", "$1");
	        if (temp.equals(s)) break;
	        s = temp;
	    }

		System.out.println(answer);
	}

}
